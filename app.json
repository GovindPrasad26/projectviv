{
  "expo": {
    "name": "homepage",
    "slug": "homepage",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "homepage",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "config": {
        "googleMapsApiKey": "AIzaSyDbhDt1_GZG_bW-3f5s6cfSTY2CepE4GEg"
      },
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "We need your location to show your delivery address.",
        "NSLocationAlwaysAndWhenInUseUsageDescription": "We need your location to provide delivery services."
      }
    },
    "android": {
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyDbhDt1_GZG_bW-3f5s6cfSTY2CepE4GEg"
        }
      },
      "permissions": [
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION",
        "INTERNET",
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION"
      ],
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.anonymous.homepage"
    },
    "web": {
      "bundler": "metro"
    },
    "plugins": [
      "expo-location"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "f3a61557-ea5d-438d-8293-d8dfda5bd2b5"
      }
    }
  }
}
